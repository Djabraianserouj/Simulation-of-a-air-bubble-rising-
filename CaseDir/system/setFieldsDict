FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.water 1
);

regions
(
       sphereToCell
       {
            centre ( 0 0.01 0); 
            radius 0.0025 ;
                fieldValues
                (
                  volScalarFieldValue alpha.water 0
                  volScalarFieldValue C 8.0
                );
       }
       
       
       
);


